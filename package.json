{
  "name": "portfolio",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "deploy": "npm run build && vcg --branch=master"
  },
  "dependencies": {
    "bootstrap-vue": "^2.0.0-rc.6",
    "chart.js": "^2.7.2",
    "font-awesome": "^4.7.0",
